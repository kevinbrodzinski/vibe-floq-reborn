  "scripts": {
    "dev": "vite",
    "build": "ROLLUP_BUNDLE=true vite build",
    "build:dev": "ROLLUP_BUNDLE=true vite build --mode development",
    "supabase:gen-types": "supabase gen types typescript --project-id $SUPABASE_REF --schema public > integrations/supabase/types.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "eas:build": "eas build",
    "eas:build:development": "eas build --profile development",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:production": "eas build --profile production",
    "eas:submit": "eas submit",
    "eas:submit:ios": "eas submit --platform ios",
    "eas:submit:android": "eas submit --platform android",
    "test": "vitest run --coverage",
    "deno:install": "curl -fsSL https://deno.land/install.sh | sh || powershell -c \"irm https://deno.land/install.ps1 | iex\"",
    "deno:check": "deno check --allow-all supabase/functions/**/*.ts || echo 'Deno not installed. Run: pnpm deno:install'",
    "deno:lint": "deno lint supabase/functions/ || echo 'Deno not installed. Run: pnpm deno:install'",
    "deno:fmt": "deno fmt supabase/functions/ || echo 'Deno not installed. Run: pnpm deno:install'",
    "deno:test": "deno test --allow-all supabase/functions/ || echo 'Deno not installed. Run: pnpm deno:install'",
    "lovable:dev": "npm run dev",
    "lovable:build": "npm run build",
    "lovable:preview": "npm run preview"
  },
