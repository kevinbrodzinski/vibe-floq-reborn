# Floq Environment Variables

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Location & Presence System
# Set to 'true' to use mock presence data in development (default: false)
# When false, the app will attempt to use real presence data from Supabase
VITE_USE_MOCK_PRESENCE=false

# Mapbox Configuration
VITE_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Feature Flags
VITE_ENABLE_DEBUG_FEATURES=false

